<div id='car-edit-div' class="container">
      <div class="row">

        <div id="profile-page-sizing">
          <div id="car-list" class="card">
            <img id="img"
              src="{{image_url}}"
              class="card-img-top"
              alt="{{make}} {{model}}"
            />
            <div id="list-items" class="card-body">
              <h5 class="card-title"><a href="/car/{{id}}">{{make}}
                  {{model}}</a></h5>  
                <li>Year: {{year}}</li>
                <li>Price: {{price}} ðŸ’²</li>
                <li>
                  Bids:
                  {{bids.length}}
                </li>
                 <li>
                 Greatest Bid:
                  {{highest_bid bids}}
  
                </li>
                <a id="edit-button-profile"
                  href="/profile/edit/{{id}}"
                  class="btn btn-sm btn-secondary"
                >Edit Car</a>
            </div>
            <div class="card-footer">
              <small  id='colour' class="text">Created by
                {{user.username}}
                on
                {{format_date created_at}}</small>
            </div>
          </div>
        </div>
      </div>
</div>

        
        
      