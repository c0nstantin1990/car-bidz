<!DOCTYPE html>
<html lang="en">

  <div id="sidebar-container"></div>

  <!-- Handlebars template -->
  <div id="sidebar-template" style="display: none;">
    <div class="container mt-4">
      <div class="row">
        <!-- Sidebar -->
        <div class="col-md-3">
          <div class="dropdown">
            <button class="btn btn-primary dropdown-toggle" type="button" id="searchByDropdown" data-bs-toggle="dropdown"
              aria-expanded="false">
              Search by:
            </button>
            <ul class="dropdown-menu" aria-labelledby="searchByDropdown">
              {{#each dropdownItems.handlebars}}
              <li><a class="dropdown-item" href="#">{{this}}</a></li>
              {{/each}}
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>